//CLAUDEMIR SOUZA - PREENCHENDO UM VETOR
programa
{
	
	funcao inicio()
	{
		inteiro vetor[8]
		escreva("===== PREENCHENDO AUTOMATICAMENTE VETOR DE 8 POSIÇÕES =====\n")
		para (inteiro i = 0; i < 8; i++)
		{
			vetor[i] = 999
			escreva("[",vetor[i],"]", " ")
		}
	}	
	
}
