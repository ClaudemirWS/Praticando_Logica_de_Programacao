//CLAUDEMIR SOUZA - TABUADA
programa
{
	
	funcao inicio()
	{
		inteiro num
		escreva("Digite o número o qual quer ver a tabuada: ")
		leia(num)
		
		para (inteiro i = 1; i <= 10; i++)
		{
			escreva("\n",num," x ", i, " = ", num*i)
		}
		
	}	
	
}
