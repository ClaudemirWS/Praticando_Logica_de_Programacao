//CLAUDEMIR SOUZA - CONTAGEM ATÉ NÚMERO LIDO
programa
{
	
	funcao inicio()
	{
		inteiro n, i = 0
		escreva("========== CONTAGEM ==========")
		escreva("\nDigte um número inteiro: ")
		leia(n)
		enquanto (i < n)
		{
			i += 1
			escreva(i, " ")
		}
		escreva("Acabou!")
		
	}	
	
}
