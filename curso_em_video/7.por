//CLAUDEMIR SOUZA - DOBRO E TERÇA PARTE
programa
{
	funcao inicio()
	{
	real num
	escreva("Digite um número: ")
	leia(num)
	escreva("O número ", num," tem como dobro ", num*2, " e terça parte ", num/3, ".") 
	}
}
