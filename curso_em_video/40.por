//CLAUDEMIR SOUZA - CONTAGEM DE 3 EM 3
programa
{
	
	funcao inicio()
	{
		inteiro c = 0
		enquanto (c < 19)
		{
			escreva(c, " ")
			c += 3
		}
		escreva("Acabou!")
		
	}	
	
}
