//CLAUDEMIR SOUZA - TABUADA
programa
{
	
	funcao inicio()
	{
		inteiro num
		escreva("Digite um número para ver a contagem até ele: ")
		leia(num)
		
		para (inteiro i = 0; i <= num; i++)
		{
			escreva(i, ", ")
		}
		escreva("Acabou!")
		
	}	
	
}
