//CLAUDEMIR SOUZA - DE 100 ATÉ 0 DE 5 EM 5
programa
{
	
	funcao inicio()
	{
		inteiro c = 105
		enquanto (c != 0)
		{
			c -= 5
			escreva(c, " ")
		}
		escreva("Acabou!")
		
	}	
	
}
